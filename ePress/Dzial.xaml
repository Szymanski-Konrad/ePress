<Window x:Class="ePress.Dzial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ePress"
        mc:Ignorable="d"
        Title="Dzial" Height="110" Width="250">
    <Grid>
        <Button x:Name="Autorzy" Content="Autorzy" Height="50" Width="100" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10" Click="Autorzy_Click"/>
        <Button x:Name="Umowy" Content="Umowy" Height="50" Width="100" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="10" Click="Umowy_Click"/>
    </Grid>
</Window>