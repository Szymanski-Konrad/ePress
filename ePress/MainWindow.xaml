<Window x:Class="ePress.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ePress"
        mc:Ignorable="d"
        Title="ePress" Height="500" Width="150">
    <StackPanel HorizontalAlignment="Center">
        <Button x:Name="Dzial" Content="Dział Programowy" Height="50" Click="Dzial_Click"/>
        <Button x:Name="Drukarnie" Content="Drukarnie" Height="50" Click="Drukarnie_Click"/>
        <Button x:Name="Zamowienia" Content="Zamówienia" Height="50" Click="Zamowienia_Click"/>
        <Button x:Name="Zapis" Content="Zapisz dane" Height="50" Click="Zapis_Click"/>
    </StackPanel>
</Window>
